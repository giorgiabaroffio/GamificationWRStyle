<?xml version="1.0" encoding="UTF-8"?>

<Descriptor service="com.webratio.units.utility.rtx.db.SelectorUnitService" name="Select Community User">
  <MaxResults/>
  <Query entity="ent15x">
    <Input id="p0" param="kcond10q.userOID" type="integer" implied="false" expandStyle="in"/>
    <Output id="userOID" name="OID" index="0"/>
    <Output id="att85x" name="isoCode" index="1"/>
